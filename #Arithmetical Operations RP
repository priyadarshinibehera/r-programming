Operations
# Arithmetical Operations(+,-,*,/,%%,%/%)

a<-c(1,2,3,4,5)# c is denoted as Vectors
b<-c(2,3,4,5)
print(a+b)#Addition
print(a-b)#Subtraction
print(a*b)#Multiplication
print(a/b)#Division
print(a%%b)#Remainder
print(a%/%b)#Co-efficient

a<-c(1L,2L,3L,4L,5L) #Integer Vectors
a<-c("priyadarshini","Rohan","Nitish","Deepthi") #Character Vectors 


