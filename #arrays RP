#arrays

friends_array<-array(data=c("priya","rani","lora","sanjana","pramila"),dim=c(3,4,5))
print(friends_array)

brothers_array<-array(data=c("rahul","rohit","rathikanth","raja","rinku"),dim=c(3,6,8))
