#Statements
# if(),else(),else if()
  
marks<-35
if(marks>=75)
{
  paste("Congratulations","You got First Class:",marks)
}else if(marks>=65)
{
  paste("Congratulations","You got Second Class:",marks)
}else if(marks>=50)
{
  paste("Congratulations","You got Third Class:",marks)
}else
{
  paste("Work Hard to pass:",marks)
} 

